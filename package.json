    "test:frontend:ui": "playwright test --ui",
    "test:frontend:headed": "playwright test --headed",
    "docs": "jsdoc -c jsdoc.conf.json",
    "docs:serve": "npm run docs && cd docs/frontend && python3 -m http.server 8080"
