    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "audit": "audit-ci --config audit-ci.json",
    "analyze": "madge --circular --extensions ts,tsx --ts-config tsconfig.json components/ app/ lib/",
